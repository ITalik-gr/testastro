          </div>

          <div class="hidden items-center z-10 w-full lg:flex">
            <MainButton title="Contact Us" link="/contact" style="w-full max-w-full h-[44px] sm:max-w-[140px]" />

            <div class="flex items-center ml-6">
              <Image class='mr-3' src={User_1} width={44} alt='User Photo' />
              <div class="text-white font-semibold">Want to know more?</div>
            </div>
            
          </div>
        </div>  
        <!-- Card -->
        <PriceCard styles="!h-[545px] mb-16 sm:!h-[679px] lg:mb-0 xl:mr-[159px]"type="from500" />
      </div>

      <div class="lg:hidden"><ServicesLink /></div>
    </div>
  </section>


  <Reviews />


  <AdditionalServices />

  <Faq />

  <FreeAudit />

  <Footer />

</MainLayout>

