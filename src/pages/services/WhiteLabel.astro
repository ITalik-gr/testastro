---
import '../../styles/main.css'
import MainLayout from "../../layouts/MainLayout.astro";

import ServicesHeader from '../../components/Headers/ServicesHeader.astro';
import MainButton from '../../components/Buttons/MainButton.astro';

import { Image } from 'astro:assets';

import AdditionalServices from '../../sections/AdditionalServices.astro'

import Reviews from '../../sections/Reviews.astro'

import OurCase from '../../sections/Base/OurCase.astro';

import FreeAudit from '../../sections/Base/FreeAudit.astro'
import Footer from '../../sections/Base/Footer.astro'

import Faq from '../../sections/Faq.astro';

import InsaneGrowthVer2 from '../../sections/InsaneGrowth/InsaneGrowthVer2.astro'

import PriceGrid from '../../images/Decorate/Price-grid.svg';

import whiteLabelMobile from '../../images/Services/WhiteLabel-Mobile.svg'
import whiteLabelDesktop from '../../images/Services/WhiteLabel-Desktop.svg'
import Solutions from '../../sections/Solutions.astro';
import ServicesLink from '../../components/ServicesLink.astro';
import User_1 from '../../images/Users/user-1.png';
import PriceCard from '../../components/Cards/PriceCard.astro';
import FreeMarketingAudit from '../../sections/FreeAudit/FreeMarketingAudit.astro';




const {} = Astro.props

---
<MainLayout>
  <ServicesHeader title="Never worry about managing ads for your clients again" descr="Eliminate the hassle of overseeing ad management for your clients forever." mobileImage={whiteLabelMobile} desktopImage={whiteLabelDesktop}>
  </ServicesHeader>

  <Solutions />

  <InsaneGrowthVer2 />

  <OurCase line={false} />

  <FreeMarketingAudit sectionStyle="lg:-mb-[100px]" decorate_2={false} />

  <!-- 1 price card -->
  <section class="pt-16 pb-10 bg-black relative overflow-hidden mb-[72px] md:mb-24 lg:py-[72px] lg:mb-[134px]">
