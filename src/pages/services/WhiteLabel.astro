    </svg>
    <Image src={PriceGrid} class='top-1/2 -translate-y-1/2 absolute hidden z-0 lg:flex' alt="Decorative grid" />
    <div class="blur-[100px] absolute top-0 -right-20 w-[217px] rounded-full h-[140px] rotate-[57deg] bg-[rgba(129,87,255,0.4)] z-0"></div>

    <div class="container xl:max-w-[1254px]">
      <!-- Content -->
      <div class="flex flex-col-center lg:flex-row lg:items-center lg:justify-between">

        <!-- Text -->
        <div class="scroll-animation lg:w-full">
          <div class="text-[28px] text-center -tracking-[0.84px] text-white leading-117 font-bold sm:text-center md:text-5xl lg:mb-10 lg:max-w-[471px] lg:text-[52px] lg:text-left">Your clients will never leave</div>
          <!-- decorate line -->
          <div class="flex justify-center">
            <svg class="my-10 lg:hidden" xmlns="http://www.w3.org/2000/svg" width="79" height="10" viewBox="0 0 79 10" fill="none">
              <g opacity="0.5">
