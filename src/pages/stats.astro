---
import '../styles/main.css'
import MainLayout from "../layouts/MainLayout.astro";
import MainButton from '../components/Buttons/MainButton.astro';
import HeaderGrid from '../images/header-grid.svg'
import { Image } from 'astro:assets';
import Nav from '../components/Nav.astro';
import OurCase from '../sections/Base/OurCase.astro';
import InsaneGrowth from '../sections/InsaneGrowth/InsaneGrowth.astro';


import Reviews from '../sections/Reviews.astro';
import Faq from '../sections/Faq.astro';
import FreeAudit from '../sections/Base/FreeAudit.astro';
import Footer from '../sections/Base/Footer.astro';
import AdditionalServices from '../sections/AdditionalServices.astro';
import BreakdownByState from '../sections/BreakdownByState.astro'
import MarketsStats from '../sections/MarketsStats.astro';
import { Dashboard } from '../sections/Dashboard.jsx';
import InfoCard from '../components/Cards/InfoCard.astro';
import Parthners from '../sections/Base/Parthners.astro';


const {} = Astro.props

---

<MainLayout>

  <header class="header bg-[#0F0F11] relative overflow-x-clip">
    <Nav />
  
    <div class="header-wrapper pb-16 pt-[124px] -mt-[1px] lg:pt-[205px]">
  
      <!-- Decorate -->
      <Image src={HeaderGrid} width={1464} height={525} alt="Header Background" 
             class="absolute z-0 -top-5 left-1/2 -translate-x-1/2 w-[1464px] h-[525px] object-cover 2xl:w-[1700px]" />
  
      <div class="absolute bg-[rgba(129,87,255,0.4)] -top-4 -right-72 md:-right-20 w-[408px] h-[149px] rounded-full blur-[184px]"></div>
      <div class="absolute bg-[rgba(129,87,255,0.4)] top-44 left-1/2 -translate-x-1/2 hidden sm:flex w-[320px] h-[120px] rounded-full blur-[184px]"></div>
  
      <!-- Stars -->
      <div class="star absolute top-44 right-6 md:right-24 md:top-[400px]">
        <svg xmlns="http://www.w3.org/2000/svg" width="29" height="29" viewBox="0 0 29 29" fill="none">
          <g opacity="0.5" clip-path="url(#clip0_840_999)">
