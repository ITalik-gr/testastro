            <clipPath id="clip0_840_999">
              <rect width="22" height="22" fill="white" transform="translate(0 8.33667) rotate(-22.2681)"/>
            </clipPath>
          </defs>
        </svg>
      </div>
  
      <!-- Content -->
    
      <div class="container">
        <div class="wrap z-20 relative flex justify-center sm:items-center flex-col items-center">
  
          <!-- Header text + btn -->
          <div class="w-full text-center flex flex-col items-center">
  
            <!-- Mobile top info link -->
            <div class="w-full flex justify-center sm:hidden z-10">
              <a href="#" class="text-sm px-[10px] py-[10px] bg-gray-900 rounded-3xl text-white flex items-center w-max mb-4 sm:hidden">
                Get a free audit. Time is limited
                <svg xmlns="http://www.w3.org/2000/svg" class="ml-[10px] flex" width="16" height="17" viewBox="0 0 16 17" fill="none">
                  <path d="M3.3335 8.49998H12.6668M12.6668 8.49998L8.00016 3.83331M12.6668 8.49998L8.00016 13.1666" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
              </a>
            </div>
      
            <!-- Descktop top info link-->
            <a href="#" class="hidden sm:flex px-[14px] py-[12px] z-10 bg-gray-800 rounded-3xl text-white items-center w-max mb-6">
              ðŸ˜Ž Hurry up and get a free audit. Time is limited
              <svg xmlns="http://www.w3.org/2000/svg" class="ml-[10px] flex" width="16" height="17" viewBox="0 0 16 17" fill="none">
                <path d="M3.3335 8.49998H12.6668M12.6668 8.49998L8.00016 3.83331M12.6668 8.49998L8.00016 13.1666" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
              </svg>
            </a>
      
            <!-- Title -->
            <h1 class="text-4xl font-bold leading-[109.5%] tracking-[-1.26px] text-white mb-4 z-10 max-w-sm
                      sm:text-center sm:text-[48px] sm:max-w-lg
                      lg:tracking-[-2.87px] lg:text-[62px] lg:max-w-[900px] ">
                      PPC Agency for Dentists in USA
            </h1>
            
            <p class="font-semibold text-white text-opacity-80 mb-6 z-10 
                      sm:text-center sm:max-w-none sm:mb-[37px]">
              Paid ad strategy that drives massive growth
            </p>
      
            <MainButton title="Get Free Audit" style="w-full max-w-sm h-[60px] mb-[72px] md:mb-[80px] sm:max-w-[203px] sm:btn-shadow" />
          </div>

          <!-- Stats numbers -->
          <div class="child:mb-4 last:m-0 w-full flex flex-col-center | 
                      md:flex-row md:child:max-w-[32%] md:child:mb-0 md:child:mr-5 lg:child:max-w-xs">

                      <InfoCard name="Avg. Search volume" value="188K" />
                      <InfoCard name="Avg. Traffic potential" value="32K" />
                      <InfoCard name="Estimated CPC" value="$3.3" />
          </div>

          <!-- Decorate -->
          <div class="mt-[62px] hidden lg:flex">
            <svg xmlns="http://www.w3.org/2000/svg" width="174" height="177" viewBox="0 0 174 177" fill="none">
              <g opacity="0.5">
                <path fill-rule="evenodd" clip-rule="evenodd" d="M87.0731 71.5064C85.2872 70.1698 83.3266 69.0287 81.1754 68.1098C73.6271 64.8838 64.0044 69.0153 57.1151 75.9945C50.2282 82.9708 46.218 92.6527 49.3169 99.9609C51.0881 104.14 53.5998 106.463 56.4318 107.493C59.2493 108.518 62.44 108.257 65.6856 107.068C74.7311 103.749 84.2704 93.0827 86.5397 86.6129C87.5878 83.6196 88.4322 80.5569 89.0925 77.4494C95.2133 84.2308 98.235 94.2609 99.3437 105.395C101.754 129.593 95.0208 159.078 89.5799 173.769C89.2876 174.558 89.6917 175.433 90.481 175.725C91.2703 176.017 92.1477 175.615 92.4401 174.826C97.9884 159.844 104.835 129.772 102.376 105.094C101.107 92.3468 97.3283 81.0495 89.7851 73.8085C93.724 50.2336 88.0969 24.0484 83.5031 1.82847C83.3359 1.00564 82.5265 0.474535 81.7014 0.64477C80.8792 0.817351 80.3474 1.62135 80.5176 2.44654C84.9223 23.7588 90.4127 48.7853 87.0731 71.5064ZM86.498 74.9317C84.5819 73.3105 82.4147 71.9523 79.9735 70.912C73.4412 68.1189 65.2458 72.0925 59.2822 78.1344C55.6283 81.8391 52.8024 86.3657 51.7359 90.801C51.0642 93.5891 51.0915 96.3332 52.1264 98.7708C53.4824 101.97 55.3077 103.842 57.4744 104.627C59.6533 105.422 62.1215 105.128 64.6339 104.204C72.8788 101.181 81.5963 91.4942 83.6612 85.6048C84.8801 82.1314 85.8101 78.5665 86.498 74.9317Z" fill="#8057FF"/>
              </g>
            </svg>
          </div>
        </div>
      </div>
    </div>
  </header>

  <!--  -->
  <MarketsStats />

  <Dashboard client:only />

  <!-- Parthners -->
  <Parthners sectionStyle="mt-16 lg:mt-[100px]" />

  <!--  -->

  <!--  -->

  <OurCase line={false} />

  <InsaneGrowth style="!mb-0" />

  <BreakdownByState />

  <Reviews />

  <AdditionalServices sectionStyle="pt-24" />

  <Faq />

  <FreeAudit />

  <Footer />

</MainLayout>

